
public class TestaContas {

	public static void main(String[] args) {
		Conta cc = new ContaCorrente();
		Conta cp = new ContaPoupanca();
		
		cc.deposita(100);
		cp.deposita(1000);
		
		cc.saca(10);
		cp.saca(100);
		
		System.out.println("Saldo: " + cc.getSaldo());
		System.out.println("Saldo: " + cp.getSaldo());
	}

}
